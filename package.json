{
  "name": "cross-platform-components",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run start",
    "start": "vite --open",
    "build": "rimraf dist && esno ./scripts/components-script.ts",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "main": "./lib/index.umd.js",
  "module": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "keywords": [
    "frontend",
    "quark",
    "web components"
  ],
  "dependencies": {
    "quarkc": "^1.0.17"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.2",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "less": "^4.1.3",
    "postcss": "^8.4.24",
    "rimraf": "^5.0.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.0",
    "vite-plugin-dts": "^3.3.1"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
